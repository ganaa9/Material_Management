{
    "sap.app": {
      "id": "material_management",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "title": "Material Management App",
      "description": "A SAP Fiori UI5 application for managing materials.",
      "dataSources": {
        "mainService": {
          "uri": "/sap/opu/odata/sap/ZMY_SERVICE_SRV/",
          "type": "OData",
          "settings": {
            "odataVersion": "2.0"
          }
        }
      }
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
        "desktop": true,
        "tablet": true,
        "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
        "viewName": "material_management.view.App",
        "type": "XML",
        "async": true
      },
      "dependencies": {
        "minUI5Version": "1.60.0",
        "libs": {
          "sap.m": {},
          "sap.ui.core": {},
          "sap.f": {}
        }
      },
      "models": {
        "": {
          "dataSource": "mainService",
          "preload": true
        }
      }
    }
  }
  